{
  "name": "srag/h5ppagecomponent",
  "description": "Add H5P contents in page component editor",
  "type": "project",
  "keywords": [
    "component",
    "h5p",
    "ilias",
    "page",
    "plugin"
  ],
  "homepage": "https://github.com/srsolutionsag/H5PPageComponent",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "sr solutions ag",
      "email": "support@sr.solutions",
      "homepage": "https://sr.solutions",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://plugins.sr.solutions/goto.php?target=uihk_srsu_PLH5P"
  },
  "require": {
    "php": ">=7.0",
    "srag/librariesnamespacechanger": ">=0.1.0"
  },
  "autoload": {
    "psr-4": {
      "srag\\Plugins\\H5PPageComponent\\": "src/"
    },
    "classmap": [
      "classes/"
    ],
    "files": [
      "../../../Repository/RepositoryObject/H5P/vendor/autoload.php"
    ]
  },
  "config": {
    "classmap-authoritative": true,
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "pre-autoload-dump": [
      "srag\\LibrariesNamespaceChanger\\LibrariesNamespaceChanger::rewriteLibrariesNamespaces",
      "srag\\LibrariesNamespaceChanger\\PHP72Backport::PHP72Backport"
    ]
  }
}
